{
  "name": "lucianoqueiroz/event-store-cakephp-orm-adapter",
  "description": "CakePHP ORM Adapter for ProophEventStore",
  "homepage": "http://github.com/lucianoqueiroz/evest-store-cakephp-orm-adapter",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Luciano Queiroz",
      "email": "luciiano.queiroz@gmail.com"
    },
    {
      "name": "Rafael Queiroz",
      "email": "rafaelfqf@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "prooph/event-store": "7.0.x-dev",
    "cakephp/orm": "^3.4"
  },
  "require-dev": {
    "sandrokeil/interop-config": "^2.0.1",
    "phpunit/phpunit": "^6.0",
    "phpspec/prophecy": "^1.7",
    "prooph/php-cs-fixer-config": "^0.1.1",
    "psr/container": "^1.0",
    "satooshi/php-coveralls": "^1.0",
    "malukenho/docheader": "^0.1.4"
  },
  "suggest": {
    "ext-pdo_mysql": "For usage with MySQL",
    "ext-pdo_pgsql": "For usage with PostgreSQL"
  },
  "conflict": {
    "sandrokeil/interop-config": "<2.0.1"
  },
  "autoload": {
    "psr-4": {
      "Prooph\\EventStore\\CakePHP\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ProophTest\\EventStore\\CakePHP\\": "tests/",
      "ProophTest\\EventStore\\": "vendor/prooph/event-store/tests/"
    }
  }
}